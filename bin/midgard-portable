#!/usr/bin/env php
<?php
// we assume installed as root for now
$project_dir = dirname(__DIR__);

require_once $project_dir . '/vendor/autoload.php';

use Symfony\Component\Console\Application;
use midgard\portable\command;

$console = new Application('midgard-portable', '1.0.0-dev');
$console->add(new command\parse);
$console->run();